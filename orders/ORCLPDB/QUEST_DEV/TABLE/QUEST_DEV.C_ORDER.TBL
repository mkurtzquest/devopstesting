DROP TABLE QUEST_DEV.C_ORDER CASCADE CONSTRAINTS;

CREATE TABLE QUEST_DEV.C_ORDER
(
  O_ID          NUMBER,
  O_D_ID        NUMBER,
  O_W_ID        NUMBER,
  O_C_ID        NUMBER,
  O_ENTRY_D     DATE,
  O_CARRIER_ID  NUMBER,
  O_OL_CNT      NUMBER,
  O_ALL_LOCAL   NUMBER
)
TABLESPACE TOOLS
PCTUSED    0
PCTFREE    10
INITRANS   1
MAXTRANS   255
STORAGE    (
            INITIAL          64K
            NEXT             1M
            MINEXTENTS       1
            MAXEXTENTS       UNLIMITED
            PCTINCREASE      0
            BUFFER_POOL      DEFAULT
           )
LOGGING 
NOCOMPRESS 
NOCACHE
MONITORING;


CREATE UNIQUE INDEX QUEST_DEV.C_ORDER_I1 ON QUEST_DEV.C_ORDER
(O_ID, O_W_ID, O_D_ID)
LOGGING
TABLESPACE TOOLS
PCTFREE    10
INITRANS   2
MAXTRANS   255
STORAGE    (
            INITIAL          64K
            NEXT             1M
            MINEXTENTS       1
            MAXEXTENTS       UNLIMITED
            PCTINCREASE      0
            BUFFER_POOL      DEFAULT
           );
