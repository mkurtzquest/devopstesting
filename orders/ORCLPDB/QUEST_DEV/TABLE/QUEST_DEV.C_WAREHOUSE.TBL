DROP TABLE QUEST_DEV.C_WAREHOUSE CASCADE CONSTRAINTS;

CREATE TABLE QUEST_DEV.C_WAREHOUSE
(
  W_ID        NUMBER,
  W_YTD       NUMBER,
  W_TAX       NUMBER,
  W_NAME      VARCHAR2(10 BYTE),
  W_STREET_1  VARCHAR2(20 BYTE),
  W_STREET_2  VARCHAR2(20 BYTE),
  W_CITY      VARCHAR2(20 BYTE),
  W_STATE     CHAR(2 BYTE),
  W_ZIP       CHAR(9 BYTE)
)
TABLESPACE TOOLS
PCTUSED    0
PCTFREE    10
INITRANS   1
MAXTRANS   255
STORAGE    (
            INITIAL          64K
            NEXT             1M
            MINEXTENTS       1
            MAXEXTENTS       UNLIMITED
            PCTINCREASE      0
            BUFFER_POOL      DEFAULT
           )
LOGGING 
NOCOMPRESS 
NOCACHE
MONITORING;


CREATE UNIQUE INDEX QUEST_DEV.C_WAREHOUSE_I1 ON QUEST_DEV.C_WAREHOUSE
(W_ID)
LOGGING
TABLESPACE TOOLS
PCTFREE    10
INITRANS   2
MAXTRANS   255
STORAGE    (
            INITIAL          64K
            NEXT             1M
            MINEXTENTS       1
            MAXEXTENTS       UNLIMITED
            PCTINCREASE      0
            BUFFER_POOL      DEFAULT
           );
