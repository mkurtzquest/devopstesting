DROP TABLE QUEST_DEV.C_ITEM CASCADE CONSTRAINTS;

CREATE TABLE QUEST_DEV.C_ITEM
(
  I_ID     NUMBER,
  I_IM_ID  NUMBER,
  I_NAME   VARCHAR2(24 BYTE),
  I_PRICE  NUMBER,
  I_DATA   VARCHAR2(50 BYTE)
)
TABLESPACE TOOLS
PCTUSED    0
PCTFREE    10
INITRANS   1
MAXTRANS   255
STORAGE    (
            INITIAL          64K
            NEXT             1M
            MINEXTENTS       1
            MAXEXTENTS       UNLIMITED
            PCTINCREASE      0
            BUFFER_POOL      DEFAULT
           )
LOGGING 
NOCOMPRESS 
NOCACHE
MONITORING;


CREATE UNIQUE INDEX QUEST_DEV.C_ITEM_I1 ON QUEST_DEV.C_ITEM
(I_ID)
LOGGING
TABLESPACE TOOLS
PCTFREE    10
INITRANS   2
MAXTRANS   255
STORAGE    (
            INITIAL          64K
            NEXT             1M
            MINEXTENTS       1
            MAXEXTENTS       UNLIMITED
            PCTINCREASE      0
            BUFFER_POOL      DEFAULT
           );
