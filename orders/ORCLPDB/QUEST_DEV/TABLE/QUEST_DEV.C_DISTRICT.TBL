DROP TABLE QUEST_DEV.C_DISTRICT CASCADE CONSTRAINTS;

CREATE TABLE QUEST_DEV.C_DISTRICT
(
  D_ID         NUMBER,
  D_W_ID       NUMBER,
  D_YTD        NUMBER,
  D_TAX        NUMBER,
  D_NEXT_O_ID  NUMBER,
  D_NAME       VARCHAR2(10 BYTE),
  D_STREET_1   VARCHAR2(20 BYTE),
  D_STREET_2   VARCHAR2(20 BYTE),
  D_CITY       VARCHAR2(20 BYTE),
  D_STATE      CHAR(2 BYTE),
  D_ZIP        CHAR(9 BYTE)
)
TABLESPACE TOOLS
PCTUSED    0
PCTFREE    10
INITRANS   1
MAXTRANS   255
STORAGE    (
            INITIAL          64K
            NEXT             1M
            MINEXTENTS       1
            MAXEXTENTS       UNLIMITED
            PCTINCREASE      0
            BUFFER_POOL      DEFAULT
           )
LOGGING 
NOCOMPRESS 
NOCACHE
MONITORING;


CREATE UNIQUE INDEX QUEST_DEV.C_DISTRICT_I1 ON QUEST_DEV.C_DISTRICT
(D_W_ID, D_ID)
LOGGING
TABLESPACE TOOLS
PCTFREE    10
INITRANS   2
MAXTRANS   255
STORAGE    (
            INITIAL          64K
            NEXT             1M
            MINEXTENTS       1
            MAXEXTENTS       UNLIMITED
            PCTINCREASE      0
            BUFFER_POOL      DEFAULT
           );
