CREATE OR REPLACE PROCEDURE QUEST_PROD.Loopproc ( INVAL NUMBER) IS
TMPVAR NUMBER;
TMPVAR2 NUMBER;
TOTAL NUMBER;

BEGIN
TMPVAR := 0;
TMPVAR2 := 0;
TOTAL := 0;
FOR LCV IN 1 .. INVAL LOOP
TOTAL := 2*TOTAL + 1 - TMPVAR2;
TMPVAR2 := TMPVAR;
TMPVAR := TOTAL;
END LOOP;
DBMS_OUTPUT.PUT_LINE('TOTAL IS: ' || TOTAL);
END Loopproc;
/
